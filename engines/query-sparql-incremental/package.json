    "comunica-sparql-incremental": "bin/query.js",
    "comunica-sparql-http-incremental": "bin/http.js",
    "comunica-dynamic-sparql-incremental": "bin/query-dynamic.js"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "components",
    "config",
    "engine-default.js",
    "lib/**/*.d.ts",
    "lib/**/*.js"
